{
  "name": "brief-ammos",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start:heroku": "node dist/server",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "npm run build:i18n-ssr",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint brief-ammos",
    "build:client-and-server-bundles-i18n": "ng run brief-ammos:build:production-fr && sw-precache --root=dist --config=precache-config.js && ng run brief-ammos:build:production-en && ng run brief-ammos:server:production ",
    "build:client-and-server-bundles": "ng build --prod && sw-precache --root=dist --config=precache-config.js && ng run brief-ammos:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:i18n-ssr": "npm run build:client-and-server-bundles-i18n && npm run compile:server ",
    "compile:server": "tsc -p server.tsconfig.json",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "^7.2.8",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/platform-server": "^7.2.8",
    "@angular/router": "~7.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.5.4",
    "gsap": "^2.1.2",
    "ionicons": "^4.5.5",
    "jquery": "^3.3.1",
    "ngx-cookieconsent": "^2.1.1",
    "ngx-page-scroll": "^6.0.0-beta.1",
    "ngx-page-scroll-core": "^6.0.0-beta.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.0-rc.1",
    "rxjs": "~6.3.3",
    "scrollmagic": "^2.0.6",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
